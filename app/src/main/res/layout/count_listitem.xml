<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <TextView
        android:id="@+id/gxtypeC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/checktypeTV"
        android:layout_gravity="center_vertical"
        android:text="管线类别"
        android:layout_weight="1">
    </TextView>

    <TextView
        android:id="@+id/mxdC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/gxtypeC"
        android:layout_gravity="center_vertical"
        android:text="明显点"
        android:layout_weight="1">
    </TextView>

    <TextView
        android:id="@+id/ybdC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/mxdC"
        android:layout_gravity="center_vertical"
        android:text="隐蔽点"
        android:layout_weight="1">
    </TextView>

    <TextView
        android:id="@+id/zsC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/ybdC"
        android:layout_gravity="center_vertical"
        android:text="总数"
        android:layout_weight="1">
    </TextView>
</LinearLayout>
